{
    "name": "wiz/payment-gateway-bundle",
    "description": "A Symfony2 Bundle to help you handle the payment requirement.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "JackLam",
            "email": "jack@wizmacau.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "lib-curl": ">=7.10.0",
        "lib-openssl": ">=0.9.6",
        "jacklam/php-tools": "dev-master"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Wiz\\PaymentGatewayBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.*-dev"
        },
        "gateways": [
            "Alipay"
        ]
    }
}